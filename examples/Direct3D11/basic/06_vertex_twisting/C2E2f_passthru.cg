struct C3E4v_Output {
  float4 position : POSITION;
  float4 color    : COLOR;
};

struct C2E2f_Output {
  float4 color : COLOR;
};

C2E2f_Output C2E2f_passthru( C3E4v_Output input )
{
  C2E2f_Output OUT;
  OUT.color = input.color;
  return OUT;
}
